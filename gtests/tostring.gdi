record Circle(x, y, r)

procedure main()
	local a, m, v, fmt

	m := map()
	m[1] := "a"
	m[2] := "b"
	a := [, 1, &pi, 6.02214129e23, "abc", &stdin, [].push, main,
		Circle(3,4,5), [1,2,3], m, !m]
	fmt := "%-12s %-12s %-24s %s"
	printf(fmt, "type", "string", "image", "write\n")
	printf(fmt, "----", "------", "-----", "-----\n")
	every v := !a do {
		printf(fmt, type(v) | "[failed]",
			string(v) | "[failed]",
			image(v) | "[failed]", "") 
		write(&stdout, v)
	}
end
