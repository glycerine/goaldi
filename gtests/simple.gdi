procedure main()
   write("hello", 47, 3.14159)
   every write (1 | 2 | 3)
   every write (4 to 6)
   write(p1())
   every write(p1())
   every write(p2())
   p3(11,12,13)
   every writes (!12 | "\n")
   every 1 to 5 do write(?0)
   every writes (" ", (!27) | "\n")
   every writes (" ", ?(!27) | "\n")
   every writes (" ", (!27 & ?100) | "\n")
end

procedure p1()
   return 7
end

procedure p2()
   suspend 7 | 8 | 9
end

procedure p3(a,b,c)
   write(a,b,c)
end
