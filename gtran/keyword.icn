# keywords.icn:  provide information about keywords to the compiler

procedure key_generator(name)
    static t
    initial {
	t := set([
		    "features",
		    "allocated",
		    "collections",
		    "regions",
		    "storage"
		    ])
    }
    fail # not going to support keywords that return multiple values in Goaldi
    return member(t, name)
end
